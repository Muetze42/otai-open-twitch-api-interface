{
    "name": "norman-huth/otai-open-twitch-api-interface-laravel",
    "type": "project",
    "description": "A webinterface to send Twitch API requests.",
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it",
            "role": "Lead Developer"
        }
    ],
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7",
        "inertiajs/inertia-laravel": "^v0.6.9",
        "laravel/framework": "^10.15",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.8",
        "laravel/tinker": "^2.8",
        "norman-huth/helpers-collection-laravel": "dev-development",
        "paquettg/php-html-parser": "^2.2",
        "socialiteproviders/twitch": "^5.3",
        "spatie/laravel-activitylog": "^4.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/app.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "functions/development.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan dependencies:change-vendors"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan dependencies:change-vendors"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan dependencies:change-vendors"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan dependencies:change-vendors"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
